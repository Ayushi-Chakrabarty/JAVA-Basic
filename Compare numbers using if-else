//Find greater/smaller/equal number using if else statements

public class Main
{
    public static void main(String[] args)
    {
        int a =10;
        int b =10;
        System.out.println("Java if else statement");
        System.out.println(" ");
        
        if(a>b)
        {
            System.out.println("a is greater than b");
        }
        else if(a==b)
        {
            System.out.println("a and b are equal");
        }
        else
        {
            System.out.println("b is greater than a");
        }
}}
