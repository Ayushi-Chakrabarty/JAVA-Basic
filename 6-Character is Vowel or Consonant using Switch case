//Vowel or consonant

import java.util.*;

class VC
{
    public static void main(String args[])
    {
        Scanner scnr = new Scanner(System.in);
        System.out.print("Enter the character: ");
        char ch = scnr.next().charAt(0);
        
        switch(ch)
        {
            case 'a':
            case 'e':
            case 'i':
            case 'o':
            case 'u':
                System.out.println(ch + " is a vowel.");
                break;
            default: 
                System.out.println(ch + " is a consonant.");
        }
    }
}
