//Data hiding

class Sample
{
    public int a;
    private int b; //if made public, error will go (data hiding concept)
    void display()
    {
        System.out.println("No1 =" + a + " No2 =" + b);
    }
}

class Main
{
    public static void main(String[] args)
    {
        Sample s = new Sample();
        s.a = 10;
        s.b = 20;
        s.display();
    }
}
