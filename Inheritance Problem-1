//Problems on Inheritance

class Employee {
    public void display() {
        System.out.println("Employee Name: XYZ");
    }
}

class Salary extends Employee {
    public void salary() {
        System.out.println("May month salary: 50000");
    }
}

class Main {
    public static void main(String[] args)
    {
        Salary sal = new Salary();
        sal.display();
        sal.salary();
    }
}

