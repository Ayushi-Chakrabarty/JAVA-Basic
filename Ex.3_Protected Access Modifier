//jAVA PROGRAM TO ILLUSTRATE 
//PROTECTED MODIFIER
package p1;

//Class A
class A
{
    protected void display()
    {
        System.out.println("Hello World");
    }
}

//Java program to illustrate 
//protected modifier

package p2;
import p1.*; //importing all classes in package p1

//Class B is a subclass of A
class B extends A
{
    public static void main(String args[])
    {
        B obj = new B();
        obj.display();
    }
}

//Output: Hello World
